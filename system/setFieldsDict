FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    U       (1 0 0);
    T       0.0;
}

zones
{
    startingZone
    {
        type        cylinder;

        point1      (0 0 -1);
        point2      (0 0  1);
        radius      0.5;

        values
        {
            T       1.0;
        }
    };
}

